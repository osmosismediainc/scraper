<div>
    <h1 class="center-align">Read the Latest from 'The Verge'</h1>
    <div class="row">
        <div class="col s3"></div>
        <h5 class="center-align col s6">Click 'Scrape' to get the latest articles</h5>
    </div>
</div>
<div class="row">
    <div class="col s2"></div>
    <div class="col left-align">
        {{#each article}}
        <div class="card-panel article-panel-group">
            <h4 class="article-title center">{{this.title}}</h4>
            <button href="/readArticle/{{this._id}}" class="waves-effect waves-light btn blue darken-4" id="btnSignUp">Read Article</button>


        </div>

        {{/each}}
    </div>
</div>